{
  "scripts": {
    "clean": "rm -rf docs/scripts/* public/scripts/*",
    "build": "tsc -p . --sourcemap",
    "build:watch": "tsc -p . -w --sourcemap",
    "pack": "webpack",
    "pack:watch": "webpack -w",
    "deploy": "rm -rf docs/* && cp public/*.* docs && tsc -p . --outDir docs/scripts && webpack --env production"
  },
